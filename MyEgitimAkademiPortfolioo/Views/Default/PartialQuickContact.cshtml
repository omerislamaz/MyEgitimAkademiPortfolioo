@using MyEgitimAkademiPortfolioo.Models

@model List<MyEgitimAkademiPortfolioo.Models.QuickContact>





<div class="modal right fade" id="DomainModal" tabindex="-1" role="dialog" aria-labelledby="DomainModalLabel2">
    <div class="modal-dialog" role="document">
        <div class="modal-content">

            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>

            @foreach (var item in Model)
            {


                <div class="modal-body">
                    <div class="modal__content">
                        <a class="navbar-brand"> <span>@item.İsimSoyisim</span></a>

                        <h2 class="logo">
                            <img src="@item.Resim" alt="Your logo" title="Your logo" style="height:100px;" />
                        </h2>
                        <p class="mt-md-3 mt-2">
                            @item.Aciklama
                        </p>

                        <img src="~/templates/iportfolio-starter/assets/images/banner.png" alt="" class="img-fluid mt-5">

                        <div class="widget-social-icons mt-sm-5 mt-4">
                            <ul class="icon-rounded address">
                                <li class="mt-3">
                                    <p><a href="tel:@item.Telefon">@item.Telefon</a></p>
                                </li>
                                <li class="mt-2">
                                    <p><a href="mailto:@item.Email">@item.Email</a></p>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>



            <div class="widget-social-icons mt-sm-5 mt-4">
                <ul class="follow-social">
                   
                    
                        <li><a href="@item.Behance" target="_blank"><span class="fa fa-behance"></span></a></li>
                        <li><a href="@item.Linkedin" target="_blank"><span class="fa fa-linkedin"></span></a></li>
                        <li><a href="@item.Facebook" target="_blank"><span class="fa fa-facebook"></span></a></li>
                        <li><a href="@item.Twiter" target="_blank"><span class="fa fa-twitter"></span></a></li>
                    
                </ul>
            </div>


            }

        </div>
        <!-- //modal-content -->
    </div>
    <!-- //modal-dialog -->
</div>
